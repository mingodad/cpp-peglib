class A { }
class Cell { value: A & iso; }
class GenericCell[X] { value: X; }

class Main {
  main() { }

  write_to_mut(cell: Cell & mut) {
    cell.value = new A;
  }

  write_to_iso(cell: Cell & iso) {
    cell.value = new A;
  }

  write_to_union(cell: Cell & (iso | mut)) {
    cell.value = new A;
  }

  write_to_generic[X](cell: GenericCell[X] & mut, value: X) {
    cell.value = value;
  }
}